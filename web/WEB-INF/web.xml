<?xml version="1.0" encoding="UTF-8"?><web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"         version="3.1">    <filter>        <filter-name>User</filter-name>        <filter-class>com.xxoo.filter.User</filter-class>    </filter>    <filter-mapping>        <filter-name>User</filter-name>        <url-pattern>/index.jsp</url-pattern>    </filter-mapping>    <servlet>        <servlet-name>Data</servlet-name>        <servlet-class>com.xxoo.servlet.Data</servlet-class>    </servlet>    <servlet>        <servlet-name>Hello</servlet-name>        <servlet-class>com.xxoo.servlet.Hello</servlet-class>    </servlet>    <servlet>        <servlet-name>Login</servlet-name>        <servlet-class>com.xxoo.servlet.Login</servlet-class>    </servlet>    <servlet>        <servlet-name>Loginout</servlet-name>        <servlet-class>com.xxoo.servlet.Loginout</servlet-class>    </servlet>    <servlet-mapping>        <servlet-name>Data</servlet-name>        <url-pattern>/data.do</url-pattern>    </servlet-mapping>    <servlet-mapping>        <servlet-name>Hello</servlet-name>        <url-pattern>/hello.do</url-pattern>    </servlet-mapping>    <servlet-mapping>        <servlet-name>Login</servlet-name>        <url-pattern>/login.do</url-pattern>    </servlet-mapping>    <servlet-mapping>        <servlet-name>Loginout</servlet-name>        <url-pattern>/loginout.do</url-pattern>    </servlet-mapping>    <error-page>        <location>/error.jsp</location>        <error-code>500</error-code>    </error-page>    <welcome-file-list>        <welcome-file>login.jsp</welcome-file>    </welcome-file-list></web-app>